{
  "version": 2,
  "builds": [
    {
      "src": "dist/meteo-app/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/meteo-app/browser",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/browser/(.*)",
      "dest": "/dist/meteo-app/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/meteo-app/server/main.js"
    }
  ]
}