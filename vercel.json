{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist/meteo-app"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/browser/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/browser/favicon.ico"
    },
    {
      "src": "/(.+\\..+)",
      "dest": "/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.mjs"
    }
  ]
}